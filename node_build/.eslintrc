{
  "env": {
    "browser": true
  },
  "extends" : "airbnb",
  "plugins" : [
    "jsx-a11y",
    "react",
    "import"
  ],
  "rules" : {
    "linebreak-style": 0,
    "object-curly-newline": ["error", {
      "ImportDeclaration": "never"
    }],
    "react/forbid-prop-types": 0,
    "react/require-default-props": 0,
    "react/jsx-props-no-spreading": 0
  }
}